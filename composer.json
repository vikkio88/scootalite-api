{
    "name": "vikkio88/scootalite-api",
    "description": "a simple RestFul API service for ScootaLite, based on SlimE",
    "keywords": ["podcast", "vikkio88", "rest", "eloquent", "podcast-api"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "vikkio88/slime-framework": "dev-master",
        "akrabat/rka-ip-address-middleware": "^0.4.0",
        "palanik/corsslim": "dev-slim3",
        "facebook/graph-sdk": "^5.4"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "files": [ "config/database.php" ],
        "psr-4": {
            "App\\Lib\\": "Lib/",
            "App\\Models\\": "Models/",
            "App\\Actions\\": "Actions/",
            "App\\Console\\": "Console/",
            "App\\Scripts\\": "scripts/Scripts"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    }
}
